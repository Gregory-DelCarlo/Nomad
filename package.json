  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gregory-DelCarlo/Nomad/issues"
  },
  "homepage": "https://github.com/Gregory-DelCarlo/Nomad#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "google-map-react": "^2.1.9",
    "google-maps-react": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.1.1",
    "mongoose": "^5.11.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "styled-components": "^5.2.1",
    "validator": "^13.5.2"
